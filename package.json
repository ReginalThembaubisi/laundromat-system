{
  "name": "laundromat-system",
  "version": "1.0.0",
  "description": "Student laundromat management system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd frontend && npm start",
